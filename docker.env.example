# Example environment variables for Docker deployment
# Copy this file to .env and fill in your actual Neon database credentials

# Main Database (Neon DB1) - Contains athletes, coaches, days, foods, meals
MAIN_DB_URL=jdbc:postgresql://your-main-db-host:5432/your-main-db-name?sslmode=require
MAIN_DB_USERNAME=your-main-db-username
MAIN_DB_PASSWORD=your-main-db-password

# Food Categories Database (Neon DB2) - Contains food_categories
FOODCATEGORY_DB_URL=jdbc:postgresql://your-foodcategory-db-host:5432/your-foodcategory-db-name?sslmode=require
FOODCATEGORY_DB_USERNAME=your-foodcategory-db-username
FOODCATEGORY_DB_PASSWORD=your-foodcategory-db-password 